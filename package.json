{
  "name": "example-angular-client",
  "version": "0.0.0",
  "private": "true",
  "type": "module",
  "scripts": {
    "clean": "npx shx rm -fr dist *.tsbuildinfo src/assets",
    "build:msg": "echo Building angular-client example:",
    "build": "npm run build:msg && npm run clean && ng build",
    "lint": "eslint . --ext .ts",
    "start": "npm run clean && ng serve",
    "watch": "npm run clean && ng build --watch --configuration development",
    "reset:repo": "git clean -f -X -d"
  },
  "dependencies": {
    "@angular/compiler": "~15.2.5",
    "@angular/core": "~15.2.5",
    "@angular/platform-browser": "~15.2.5",
    "@angular/platform-browser-dynamic": "~15.2.5",
    "monaco-languageclient": "~6.0.3",
    "monaco-editor-workers": "~0.37.0",
    "normalize-url": "~8.0.0",
    "rxjs": "~7.8.0",
    "vscode-ws-jsonrpc": "~3.0.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "~15.0.0",
    "@angular-devkit/build-angular": "~15.2.4",
    "@angular/cli": "~15.2.4",
    "@angular/compiler-cli": "~15.2.5",
    "@typescript-eslint/eslint-plugin": "~5.59.7",
    "@typescript-eslint/parser": "~5.59.7",
    "@types/node": "~18.16.14",
    "@types/vscode": "~1.78.0",
    "css-loader": "~6.7.4",
    "editorconfig": "~1.0.2",
    "eslint": "~8.41.0",
    "eslint-config-standard": "~17.0.0",
    "eslint-plugin-header": "~3.1.1",
    "eslint-plugin-import": "~2.27.5",
    "eslint-plugin-promise": "~6.1.1",
    "eslint-plugin-unused-imports": "~2.0.0",
    "path-browserify": "~1.0.1",
    "shx": "~0.3.4",
    "style-loader": "~3.3.3",
    "tslib": "~2.5.2",
    "typescript": "~4.9.5"
  },
  "volta": {
    "node": "18.16.0",
    "npm": "9.6.6"
  }
}
