{
  "name": "example-angular-client",
  "version": "0.0.0",
  "private": "true",
  "type": "module",
  "scripts": {
    "clean": "shx rm -fr dist *.tsbuildinfo src/assets",
    "build:msg": "echo Building angular-client example:",
    "build": "npm run build:msg && npm run clean && ng build",
    "lint": "eslint . --ext .ts",
    "start": "npm run clean && ng serve",
    "start:server": "node --loader ts-node/esm ./src/server/json-server.ts",
    "watch": "npm run clean && ng build --watch --configuration development",
    "reset:repo": "git clean -f -X -d"
  },
  "dependencies": {
    "@angular/compiler": "~16.2.7",
    "@angular/core": "~16.2.7",
    "@angular/platform-browser": "~16.2.7",
    "@angular/platform-browser-dynamic": "~16.2.7",
    "monaco-editor-workers": "~0.43.0",
    "monaco-languageclient-examples": "~6.5.3",
    "rxjs": "~7.8.1",
    "vscode-ws-jsonrpc": "~3.0.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "~16.0.1",
    "@angular-devkit/build-angular": "~16.2.4",
    "@angular/cli": "~16.2.4",
    "@angular/compiler-cli": "~16.2.7",
    "@typescript-eslint/eslint-plugin": "~6.7.3",
    "@typescript-eslint/parser": "~6.7.3",
    "@types/node": "~18.18.0",
    "@types/react": "~18.2.28",
    "@types/vscode": "~1.82.0",
    "css-loader": "~6.8.1",
    "editorconfig": "~2.0.0",
    "eslint": "~8.50.0",
    "eslint-config-standard": "~17.1.0",
    "eslint-plugin-header": "~3.1.1",
    "eslint-plugin-import": "~2.28.1",
    "eslint-plugin-promise": "~6.1.1",
    "eslint-plugin-unused-imports": "~3.0.0",
    "shx": "~0.3.4",
    "style-loader": "~3.3.3",
    "ts-node": "~10.9.1",
    "tslib": "~2.6.2",
    "typescript": "~5.1.6"
  },
  "volta": {
    "node": "18.18.0",
    "npm": "9.8.1"
  }
}
